{
  "name": "@fab1o/node-utility",
  "version": "1.8.7",
  "description": "A swiss-knife Library for Node applications.",
  "main": "index.js",
  "author": {
    "name": "Fabio Costa",
    "url": "https://github.com/fab1o"
  },
  "private": false,
  "scripts": {
    "start": "node ./index.js",
    "docs": "jsdoc2md src/index.js src/console/index.js src/fs/index.js src/git/index.js src/object/index.js src/shell/index.js src/string/index.js > docs.md",
    "test": "jest --runInBand",
    "lint": "eslint . --ext .js",
    "lint-fix": "prettier --ignore-path \".prettierignore\" --write \"./**/*.{js,ts,json,md}\" && eslint . --ext .js --fix",
    "dts": "npx -p typescript tsc index.js --declaration --allowJs --emitDeclarationOnly --outDir types"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "bugs": "https://github.com/fab1o/node-utility/issues",
  "homepage": "https://github.com/fab1o/node-utility",
  "engines": {
    "node": ">= 10"
  },
  "devDependencies": {
    "@fab1o/eslint-config-base": "1.3.2",
    "jest": "29.6.1",
    "jsdoc-to-markdown": "8.0.0"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "inquirer": "8.2.2",
    "semver": "7.3.6",
    "yargs": "17.4.0"
  },
  "license": "MIT",
  "types": "types/index.d.ts"
}
